<?xml version="1.0" encoding="UTF-8"?>
<notifications xmlns="http://xmlns.opennms.org/xsd/notifications">
    <ns1:header xmlns:ns1="http://xmlns.opennms.org/xsd/types">
        <rev xmlns="">1.2</rev>
        <created xmlns="">Friday, March 18, 2011 10:36:49 PM GMT</created>
        <mstation xmlns="">localhost</mstation>
    </ns1:header>
    <notification name="interfaceDown" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/nodes/interfaceDown</uei>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">All services are down on interface %interfaceresolve% (%interface%) 
on node %nodelabel%.  New Outage records have been created 
and service level availability calculations will be impacted 
until this outage is resolved.  
	</text-message>
        <subject xmlns="">Notice #%noticeid%: %interfaceresolve% (%interface%) on node %nodelabel% down.</subject>
        <numeric-message xmlns="">111-%noticeid%</numeric-message>
    </notification>
    <notification name="nodeDown" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/nodes/nodeDown</uei>
        <rule xmlns="">(IPADDR != '0.0.0.0')</rule>
        <destinationPath xmlns="">BackboneNotification</destinationPath>
        <text-message xmlns="">All services are down on node %nodelabel%.  New Outage records have &#xd;
been created and service level availability calculations will &#xd;
be impacted until this outage is resolved.  &#xd;
	</text-message>
        <subject xmlns="">Notice #%noticeid%: node %nodelabel% down.</subject>
        <numeric-message xmlns="">111-%noticeid%</numeric-message>
    </notification>
    <notification name="nodeLostService" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/nodes/nodeLostService</uei>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">The %service% service poll on interface %interfaceresolve% (%interface%) 
on node %nodelabel% failed at %time%. 
	</text-message>
        <subject xmlns="">Notice #%noticeid%: %service% down on %interfaceresolve% (%interface%) on node %nodelabel%.</subject>
        <numeric-message xmlns="">111-%noticeid%</numeric-message>
    </notification>
    <notification name="nodeAdded" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/nodes/nodeAdded</uei>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">OpenNMS has discovered a new node named
%parm[nodelabel]%. Please be advised.</text-message>
        <subject xmlns="">Notice #%noticeid%: %parm[nodelabel]% discovered.</subject>
        <numeric-message xmlns="">111-%noticeid%</numeric-message>
    </notification>
    <notification name="interfaceDeleted" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/nodes/interfaceDeleted</uei>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">Due to extended downtime or operator action, the interface %interfaceresolve% (%interface%) 
on node %nodelabel% has been deleted from OpenNMS's polling database.</text-message>
        <subject xmlns="">Notice #%noticeid%: [OpenNMS] %interfaceresolve% (%interface%) on node %nodelabel% deleted.</subject>
        <numeric-message xmlns="">111-%noticeid%</numeric-message>
    </notification>
    <notification name="High Threshold" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/threshold/highThresholdExceeded</uei>
        <description xmlns="">A monitored device has hit a high threshold</description>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">A Threshold has been exceeded on node: %nodelabel%, interface:%interface%. The parameter %parm[ds]% reached a value of %parm[value]% while the threshold is %parm[threshold]%. This alert will be rearmed when %parm[ds]% reaches %parm[rearm]%.</text-message>
        <subject xmlns="">Notice #%noticeid%: High Threshold for %parm[ds]% on node %nodelabel%.</subject>
    </notification>
    <notification name="Low Threshold" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/threshold/lowThresholdExceeded</uei>
        <description xmlns="">A monitored device has hit a low threshold</description>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">A Threshold has been exceeded on node: %nodelabel%, interface:%interface%. The parameter %parm[ds]% reached a value of %parm[value]% while the threshold is %parm[threshold]%. This alert will be rearmed when %parm[ds]% reaches %parm[rearm]%.</text-message>
        <subject xmlns="">Notice #%noticeid%: Low Threshold for %parm[ds]% on node %nodelabel%.</subject>
    </notification>
    <notification name="Low Threshold Rearmed" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/threshold/lowThresholdRearmed</uei>
        <description xmlns="">A monitored device has recovered from a low threshold</description>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">A Threshold has returned to normal on node: %nodelabel%, interface:%interface%. The parameter %parm[ds]% reached a value of %parm[value]% with a rearm threshold of %parm[rearm]%. This threshold for this alert was %parm[threshold]%.</text-message>
        <subject xmlns="">Notice #%noticeid%: Low Threshold Rearmed for %parm[ds]% on node %nodelabel%.</subject>
    </notification>
    <notification name="High Threshold Rearmed" status="off" writeable="yes">
        <uei xmlns="">uei.opennms.org/threshold/highThresholdRearmed</uei>
        <description xmlns="">A monitored device has recovered from a high threshold</description>
        <rule xmlns="">IPADDR != '0.0.0.0'</rule>
        <destinationPath xmlns="">Email-Admin</destinationPath>
        <text-message xmlns="">A Threshold has returned to normal on node: %nodelabel%, interface:%interface%. The parameter %parm[ds]% reached a value of %parm[value]% with a rearm threshold of %parm[rearm]%. This threshold for this alert was %parm[threshold]%.</text-message>
        <subject xmlns="">Notice #%noticeid%: High Threshold Rearmed for %parm[ds]% on node %nodelabel%.</subject>
    </notification>
