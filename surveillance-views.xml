<?xml version="1.0" encoding="UTF-8"?>
<surveillance-view-configuration 
  xmlns:this="http://www.opennms.org/xsd/config/surveillance-views" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.opennms.org/xsd/config/surveillance-views http://www.opennms.org/xsd/config/surveillance-views.xsd"
  default-view="default" >
  <views >
    <view name="default" refresh-seconds="300" >
      <rows>
        <row-def label="Mikrotik" >
          <category name="Mikrotik"/>
        </row-def>
        <row-def label="Ubiquiti" >
          <category name="Ubiquiti" />
        </row-def>
      </rows>
      <columns>
        <column-def label="Backbone" >
          <category name="Backbone" />
        </column-def>
      </columns>
    </view>
  </views>
</surveillance-view-configuration>
